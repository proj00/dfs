{
  "name": "dfs",
  "productName": "dfs",
  "version": "1.0.0",
  "main": ".webpack/main",
  "scripts": {
    "start": "npm run publish-backend-debug && npm run fix-proto && npx electron-forge start",
    "package": "npm run publish-backend-release && npm run fix-proto && npx electron-forge package",
    "make": "npm run publish-backend-release && npm run fix-proto && npx electron-forge make",
    "publish": "npm run publish-backend-release && npm run fix-proto && npx electron-forge publish",
    "debug-no-build": "npx electron-forge start --inspect-electron",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test-build": "npm run fix-proto && npx tsc -b",
    "package-debug": "npm run publish-backend-debug && npm run fix-proto && npx electron-forge package",
    "publish-backend-debug": "powershell -ExecutionPolicy Bypass -File ./build_backend_debug.ps1",
    "publish-backend-release": "powershell -ExecutionPolicy Bypass -File ./build_backend_release.ps1",
    "get-descriptor": "npx pbjs -t json -r -o src/types/descriptors.json ./../common/proto/rpc/uiservice.proto -p ./../common/proto",
    "generate-rpc": "npx buf generate \"../common/proto\"",
    "generate-wrapper": "ts-node --compiler-options \"{\\\"noUnusedLocals\\\": false}\" utils/codegen.ts",
    "fix-proto": "npm run generate-rpc && npm run get-descriptor && npm run generate-wrapper"
  },
  "author": {
    "name": "http500"
  },
  "description": "university course project",
